      Preprocessor="%(Yasm_Tool.Preprocessor)"
      ObjectFormat="%(Yasm_Tool.ObjectFormat)"
      DebuggingFormat="%(Yasm_Tool.DebuggingFormat)"
      ListFormat="%(Yasm_Tool.ListFormat)"
      Machine="%(Yasm_Tool.Machine)"
      MessageStyle="%(Yasm_Tool.MessageStyle)"
      AdditionalOptions="%(Yasm_Tool.AdditionalOptions)"
      Inputs="@(Yasm_Tool)" />
  </Target>
  <PropertyGroup>
    <ComputeLinkInputsTargets>
            $(ComputeLinkInputsTargets);
            ComputeYasm_ToolOutput;
          </ComputeLinkInputsTargets>
    <ComputeLibInputsTargets>
            $(ComputeLibInputsTargets);
            ComputeYasm_ToolOutput;
          </ComputeLibInputsTargets>
  </PropertyGroup>
  <Target
    Name="ComputeYasm_ToolOutput"
    Condition="'@(Yasm_Tool)' != ''">
    <ItemGroup>
      <Yasm_ToolDirsToMake
        Condition="'@(Yasm_Tool)' != '' and '%(Yasm_Tool.ExcludedFromBuild)' != 'true'"
        Include="%(Yasm_Tool.Outputs)" />
      <Link
        Include="%(Yasm_ToolDirsToMake.Identity)"
        Condition="'%(Extension)'=='.obj' or '%(Extension)'=='.res' or '%(Extension)'=='.rsc' or '%(Extension)'=='.lib'" />
      <Lib
        Include="%(Yasm_ToolDirsToMake.Identity)"
        Condition="'%(Extension)'=='.obj' or '%(Extension)'=='.res' or '%(Extension)'=='.rsc' or '%(Extension)'=='.lib'" />
      <ImpLib
        Include="%(Yasm_ToolDirsToMake.Identity)"
        Condition="'%(Extension)'=='.obj' or '%(Extension)'=='.res' or '%(Extension)'=='.rsc' or '%(Extension)'=='.lib'" />
    </ItemGroup>
    <MakeDir
      Directories="@(Yasm_ToolDirsToMake-&gt;'%(RootDir)%(Directory)')" />
  </Target>
</Project>