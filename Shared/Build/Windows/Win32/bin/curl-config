prefix=/C/Build/OSSBuild/Build/Windows/Win32/Release
exec_prefix=${prefix}
includedir=/C/Build/OSSBuild/Build/Windows/Win32/Release/include

usage()
{
    cat <<EOF
Usage: curl-config [OPTION]

Available values for OPTION include:

  --built-shared says 'yes' if libcurl was built shared
  --ca        ca bundle install path
  --cc        compiler
  --cflags    pre-processor and compiler flags
  --checkfor [version] check for (lib)curl of the specified version
  --configure the arguments given to configure when building curl
  --features  newline separated list of enabled features
  --help      display this help and exit
  --libs      library linking information
  --prefix    curl install prefix
  --protocols newline separated list of enabled protocols
  --static-libs static libcurl library linking information
  --version   output version information
  --vernum    output the version information as a number (hexadecimal)
EOF

    exit $1
}

if test $# -eq 0; then
    usage 1
fi

while test $# -gt 0; do
    case "$1" in
    # this deals with options in the style
    # --option=value and extracts the value part
    # [not currently used]
    -*=*) value=`echo "$1" | sed 's/[-_a-zA-Z0-9]*=//'` ;;
    *) value= ;;
    esac

    case "$1" in
    --built-shared)
        echo yes
        ;;

    --ca)
	echo ""
	;;

    --cc)
	echo "gcc"
	;;

    --prefix)
	echo "$prefix"
	;;

    --feature|--features)
        for feature in SSL libz NTLM ""; do
            test -n "$feature" && echo "$feature"
        done
	;;

    --protocols)
        for protocol in DICT FILE FTP FTPS HTTP HTTPS IMAP IMAPS LDAP POP3 POP3S RTSP SMTP SMTPS TELNET TFTP; do
            echo "$protocol"
        done
	;;

    --version)
	echo libcurl 7.21.1
	exit 0
	;;

    --checkfor)
        checkfor=$2
        cmajor=`echo $checkfor | cut -d. -f1`
        cminor=`echo $checkfor | cut -d. -f2`
        # when extracting the patch part we strip off everything after a
	# dash as that's used for things like version 1.2.3-CVS
	cpatch=`echo $checkfor | cut -d. -f3 | cut -d- -f1`
        checknum=`echo "$cmajor*256*256 + $cminor*256 + ${cpatch:-0}" | bc`
        numuppercase=`echo 071501 | tr 'a-f' 'A-F'`
        nownum=`echo "obase=10; ibase=16; $numuppercase" | bc`

	if test "$nownum" -ge "$checknum"; then
	  # silent success
	  exit 0
	else
	  echo "requested version $checkfor is newer than existing 7.21.1"
	  exit 1
	fi
	;;

    --vernum)
	echo 071501
	exit 0
	;;

    --help)
	usage 0
	;;

    --cflags)
       	if test "X/C/Build/OSSBuild/Build/Windows/Win32/Release/include" = "X/usr/include"; then
          echo ""
        else
          echo "-I/C/Build/OSSBuild/Build/Windows/Win32/Release/include"
        fi
       	;;

    --libs)
	if test "X/C/Build/OSSBuild/Build/Windows/Win32/Release/lib" != "X/usr/lib" -a "X/C/Build/OSSBuild/Build/Windows/Win32/Release/lib" != "X/usr/lib64"; then
	   CURLLIBDIR="-L/C/Build/OSSBuild/Build/Windows/Win32/Release/lib "
	else
	   CURLLIBDIR=""
	fi
	if test "Xyes" = "Xyes"; then
	  echo ${CURLLIBDIR}-lcurl -Wl,--enable-auto-image-base -Wl,--enable-auto-import -Wl,--enable-runtime-pseudo-reloc-v2 -Wl,--kill-at -Wl,--exclude-libs=libintl.a -Wl,--add-stdcall-alias -L/C/Build/OSSBuild/Build/Windows/Win32/Release/lib -L/C/Build/OSSBuild/Shared/Build/Windows/Win32/lib -ltasn1 -lz -lgcrypt -lgpg-error -Lc:/Build/OSSBuild/Build/Windows/Win32/Release/lib -lwldap32 -lws2_32 -lz -lgnutls -lws2_32 
	else
	  echo ${CURLLIBDIR}-lcurl -Wl,--enable-auto-image-base -Wl,--enable-auto-import -Wl,--enable-runtime-pseudo-reloc-v2 -Wl,--kill-at -Wl,--exclude-libs=libintl.a -Wl,--add-stdcall-alias -L/C/Build/OSSBuild/Build/Windows/Win32/Release/lib -L/C/Build/OSSBuild/Shared/Build/Windows/Win32/lib -ltasn1 -lz -lgcrypt -lgpg-error -Lc:/Build/OSSBuild/Build/Windows/Win32/Release/lib 
	fi
	;;

    --static-libs)
	echo /C/Build/OSSBuild/Build/Windows/Win32/Release/lib/libcurl.a -Wl,--enable-auto-image-base -Wl,--enable-auto-import -Wl,--enable-runtime-pseudo-reloc-v2 -Wl,--kill-at -Wl,--exclude-libs=libintl.a -Wl,--add-stdcall-alias -L/C/Build/OSSBuild/Build/Windows/Win32/Release/lib -L/C/Build/OSSBuild/Shared/Build/Windows/Win32/lib -ltasn1 -lz -lgcrypt -lgpg-error -Lc:/Build/OSSBuild/Build/Windows/Win32/Release/lib -lwldap32 -lws2_32 -lz -lgnutls -lws2_32 
	;;

    --configure)
      echo " '--with-gnutls' '--enable-optimize' '--disable-curldebug' '--disable-debug' '--disable-static' '--enable-shared' '--host=' '--build=' '--prefix=/C/Build/OSSBuild/Build/Windows/Win32/Release' '--libexecdir=/C/Build/OSSBuild/Build/Windows/Win32/Release/bin' '--bindir=/C/Build/OSSBuild/Build/Windows/Win32/Release/bin' '--libdir=/C/Build/OSSBuild/Build/Windows/Win32/Release/lib' '--includedir=/C/Build/OSSBuild/Build/Windows/Win32/Release/include' 'build_alias=' 'host_alias=' 'CFLAGS= -m32 -mms-bitfields -pipe -DWINVER=0x0501  -I/C/Build/OSSBuild/Build/Windows/Win32/Release/include -I/C/Build/OSSBuild/Shared/Build/Windows/Win32/include' 'LDFLAGS=-Wl,--enable-auto-image-base -Wl,--enable-auto-import -Wl,--enable-runtime-pseudo-reloc-v2 -Wl,--kill-at  -Wl,--exclude-libs=libintl.a -Wl,--add-stdcall-alias -L/C/Build/OSSBuild/Build/Windows/Win32/Release/lib -L/C/Build/OSSBuild/Shared/Build/Windows/Win32/lib -ltasn1 -lz -lgcrypt -lgpg-error' 'CPPFLAGS= -DMINGW64 -D__MINGW64__ -DMINGW32 -D__MINGW32__ -I/C/Build/OSSBuild/Build/Windows/Win32/Release/include -I/C/Build/OSSBuild/Shared/Build/Windows/Win32/include'"
    ;;

    *)
        echo "unknown option: $1"
	usage 1
	;;
    esac
    shift
done

exit 0
