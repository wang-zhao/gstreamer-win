<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets">
  </ImportGroup>
  <PropertyGroup Label="UserMacros">
    <MP3LameMajorVersion>3</MP3LameMajorVersion>
    <MP3LameMinorVersion>98</MP3LameMinorVersion>
    <MP3LameMicroVersion>2</MP3LameMicroVersion>
    <MP3LameVersionString>$(MP3LameMajorVersion).$(MP3LameMinorVersion).$(MP3LameMicroVersion)</MP3LameVersionString>
  </PropertyGroup>
  <PropertyGroup>
    <_ProjectFileVersion>12.0.30501.0</_ProjectFileVersion>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <Link>
      <AdditionalDependencies>mp3lame.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="MP3LameMajorVersion">
      <Value>$(MP3LameMajorVersion)</Value>
    </BuildMacro>
    <BuildMacro Include="MP3LameMinorVersion">
      <Value>$(MP3LameMinorVersion)</Value>
    </BuildMacro>
    <BuildMacro Include="MP3LameMicroVersion">
      <Value>$(MP3LameMicroVersion)</Value>
    </BuildMacro>
    <BuildMacro Include="MP3LameVersionString">
      <Value>$(MP3LameVersionString)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>