<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="12.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets">
  </ImportGroup>
  <PropertyGroup Label="UserMacros">
    <XvidCoreMajorVersion>1</XvidCoreMajorVersion>
    <XvidCoreMinorVersion>2</XvidCoreMinorVersion>
    <XvidCoreMicroVersion>2</XvidCoreMicroVersion>
    <XvidCoreVersionString>$(XvidCoreMajorVersion).$(XvidCoreMinorVersion).$(XvidCoreMicroVersion)</XvidCoreVersionString>
    <XvidCoreApiVersionString>$(XvidCoreMajorVersion).$(XvidCoreMinorVersion)</XvidCoreApiVersionString>
  </PropertyGroup>
  <PropertyGroup>
    <_ProjectFileVersion>12.0.30501.0</_ProjectFileVersion>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <Link>
      <AdditionalDependencies>xvidcore.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="XvidCoreMajorVersion">
      <Value>$(XvidCoreMajorVersion)</Value>
    </BuildMacro>
    <BuildMacro Include="XvidCoreMinorVersion">
      <Value>$(XvidCoreMinorVersion)</Value>
    </BuildMacro>
    <BuildMacro Include="XvidCoreMicroVersion">
      <Value>$(XvidCoreMicroVersion)</Value>
    </BuildMacro>
    <BuildMacro Include="XvidCoreVersionString">
      <Value>$(XvidCoreVersionString)</Value>
    </BuildMacro>
    <BuildMacro Include="XvidCoreApiVersionString">
      <Value>$(XvidCoreApiVersionString)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>