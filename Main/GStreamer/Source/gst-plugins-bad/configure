    { { $as_echo "$as_me:${as_lineno-$LINENO}: \$PKG_CONFIG --exists --print-errors \"gio-2.0 >= 2.16\""; } >&5
  ($PKG_CONFIG --exists --print-errors "gio-2.0 >= 2.16") 2>&5
